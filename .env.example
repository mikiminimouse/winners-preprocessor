# Webhook секрет для подписи запросов
WEBHOOK_SECRET=change-me-secret

# Расписание запуска scheduler (cron format)
# По умолчанию: каждые 15 минут
SCHEDULE_CRON=*/15 * * * *

# Пароль для PostgreSQL
POSTGRES_PASSWORD=change-me-password

# Лимиты для распаковки архивов
MAX_UNPACK_SIZE_MB=500
MAX_FILES_IN_ARCHIVE=1000

# ============================================
# MongoDB Remote (Удаленная база протоколов)
# ============================================
# Адреса MongoDB хостов для чтения протоколов закупок
# Через VPN: внутренний IP адрес MongoDB сервера
# Формат: host:port
MONGO_SERVER=192.168.0.46:8635

# Пользователь с правами чтения протоколов (только чтение)
MONGO_USER=readProtocols223

# Пароль пользователя для чтения протоколов (ЗАМЕНИТЕ НА РЕАЛЬНЫЙ!)
MONGO_PASSWORD=your-readonly-password

# Путь к SSL сертификату для безопасного подключения к удаленной БД
MONGO_SSL_CERT=/root/winners_preprocessor/certs/sber2.crt

# Максимальное количество протоколов для синхронизации за один раз
protocolsCountLimit=500

# ============================================
# MongoDB Local (Локальная база метаданных)
# ============================================
# Адрес локальной MongoDB для записи обработанных данных
# По умолчанию: localhost:27017 (для Docker контейнера)
LOCAL_MONGO_SERVER=localhost:27017

# Пользователь с правами записи в локальную базу данных
MONGO_METADATA_USER=admin

# Пароль пользователя для локальной БД (ЗАМЕНИТЕ НА РЕАЛЬНЫЙ!)
MONGO_METADATA_PASSWORD=your-local-password

# База данных для хранения метаданных обработки документов
MONGO_METADATA_DB=docling_metadata

# Коллекция для хранения синхронизированных протоколов
MONGO_METADATA_COLLECTION=protocols

# ============================================
# Устаревшие параметры (сохраняются для совместимости)
# ============================================
# Следующие параметры устарели, но могут использоваться старым кодом
mongoServer=192.168.0.46:8635
readAllUser=readProtocols223
readAllPassword=your-readonly-password
sslCertPath=/root/winners_preprocessor/certs/sber2.crt
MONGO_PROTOCOLS_DB=protocols223
MONGO_PROTOCOLS_COLLECTION=purchaseProtocol

# Лимит количества протоколов за один запрос (устаревший)
PROTOCOLS_COUNT_LIMIT=100

# ============================================
# Cloud.RU ML Inference конфигурация
# ============================================
# Публичный URL endpoint инференса
# Пример: https://5bf2f0a2-xxxx-xxxx-xxxx-xxxxxxxxxxxx.inference.cloud.ru
CLOUD_RU_INFERENCE_URL=

# IAM Access Key ID (из сервисного аккаунта Cloud.RU)
# Получить: Cloud.RU → IAM → Сервисные аккаунты → Ключи → Создать ключ (HMAC/AccessKey)
CLOUD_RU_IAM_KEY_ID=

# IAM Secret Key (сохранить сразу после создания!)
CLOUD_RU_IAM_SECRET=

# Название инференса (опционально, используется для вызова модели)
# Можно найти в Cloud.RU → ML Inference → Model RUN
CLOUD_RU_INFERENCE_NAME=



